#include <stdio.h>

int main()
{
  int m=2;
    double massiv[m][m];
    
    for (int i=0; i<m; i++)
        for (int j=0; j<m; j++)
        {
            printf("massiv[%d][%d] = ", i+1, j+1);
            scanf("%lf", &massiv[i][j]);
        }
        printf("Matritca: ");
        for (int i = 0; i < m; i++)
    {
        for (int j = 0; j < m; j++)
        {
            printf("%f  ", massiv[i][j]);
        }
        printf("\n");
    } 
  return 0;
}
